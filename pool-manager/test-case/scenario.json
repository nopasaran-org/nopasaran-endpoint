{
    "id": "GUARDS-FSM",
    "initial": "First state",
    "states": {
        "First state": {
            "on": {
                "STARTED": {
                    "target": "State Before Guards"
                }
            }
        },
        "State Before Guards": {
            "entry": [
                {
                    "type": "set (true) (value-A)"
                },
                {
                    "type": "set (false) (value-B)"
                },
                {
                    "type": "done"
                }
            ],
            "on": {
                "DONE": [
                    {
                        "target": "State Not Reached",
                        "cond": "equal (value-A value-B)"
                    },
                    {
                        "target": "State Reached"
                    }
                ]
            }
        },
        "State Not Reached": {},
        "State Reached": {}
    }
}